@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<title>Material Dash</title>
	<!-- plugins:css -->
	<link rel="stylesheet" href="/Dashboard/template/assets/vendors/mdi/css/materialdesignicons.min.css">
	<link rel="stylesheet" href="/Dashboard/template/assets/vendors/css/vendor.bundle.base.css">
	<!-- endinject -->
	<!-- Plugin css for this page -->
	<link rel="stylesheet" href="/Dashboard/template/assets/vendors/flag-icon-css/css/flag-icon.min.css">
	<link rel="stylesheet" href="/Dashboard/template/assets/vendors/jvectormap/jquery-jvectormap.css">
	<!-- End plugin css for this page -->
	<!-- Layout styles -->
	<link rel="stylesheet" href="/Dashboard/template/assets/css/demo/style.css">
	<!-- End layout styles -->
	<link rel="shortcut icon" href="/Dashboard/template/assets/images/favicon.png" />
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
</head>
<body>
	<script src="/Dashboard/template/assets/js/preloader.js"></script>
	<div class="body-wrapper">


		<!-- partial -->
		<div class="main-wrapper mdc-drawer-app-content">

			<!-- partial:partials/_navbar.html -->
			<header class="mdc-top-app-bar">
				<div class="mdc-top-app-bar__row">
					<div class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
						
						<span class="mdc-top-app-bar__title">Global Snapshot: Crypto, Currency, News & Weather</span>
					</div>

				</div>
			</header>

			<!-- partial -->
			<div class="page-wrapper mdc-toolbar-fixed-adjust">
				<main class="content-wrapper">
					<div class="mdc-layout-grid">
						<div class="mdc-layout-grid__inner">


							
							@await Component.InvokeAsync("_USDToTRYComponentPartial")
							@await Component.InvokeAsync("_EURToTRYComponentPartial")
							@await Component.InvokeAsync("_GBPToTRYComponentPartial")
							@await Component.InvokeAsync("_JPYToTRYComponentPartial")
							

							
							@await Component.InvokeAsync("_BitcoinToUSDComponentPartial")
							@await Component.InvokeAsync("_EthereumToUSDComponentPartial")
							@await Component.InvokeAsync("_DogecoinToUSDComponentPartial") 
						    @await Component.InvokeAsync("_LitecoinToUSDComponentPartial")


							
							@await Component.InvokeAsync("_FuelPriceComponentPartial")
							@await Component.InvokeAsync("_MotorinPriceComponentPartial")
							@await Component.InvokeAsync("_LPGPriceComponentPartial")

							
							@await Component.InvokeAsync("_WeatherForIstanbulComponentPartial")
							@await Component.InvokeAsync("_WeatherForAnkaraComponentPartial")
							@await Component.InvokeAsync("_WeatherForIzmirComponentPartial")

						


							@await Component.InvokeAsync("_NewsComponentPartial")

							@await Component.InvokeAsync("_DailyFoodComponentPartial")

						
						</div>
					</div>
				</main>
			
				<!-- partial -->
			</div>
		</div>
	</div>


	<!-- plugins:js -->
	<script src="/Dashboard/template/assets/vendors/js/vendor.bundle.base.js"></script>
	<!-- endinject -->
	<!-- Plugin js for this page-->
	<script src="/Dashboard/template/assets/vendors/chartjs/Chart.min.js"></script>
	<script src="/Dashboard/template/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
	<script src="/Dashboard/template/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
	<!-- End plugin js for this page-->
	<!-- inject:js -->
	<script src="/Dashboard/template/assets/js/material.js"></script>
	<script src="/Dashboard/template/assets/js/misc.js"></script>
	<!-- endinject -->
	<!-- Custom js for this page-->
	<script src="/Dashboard/template/assets/js/dashboard.js"></script>
	<!-- End custom js for this page-->


</body>
</html> 