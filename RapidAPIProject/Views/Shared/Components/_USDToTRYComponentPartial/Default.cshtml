@model RapidAPIProject.Models.USDToTRYViewModel

<div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet">
	<div class="mdc-card info-card info-card--success">
		<div class="card-inner">
			<h5 class="card-title">USD/TRY</h5>
			<h5 class="font-weight-light pb-2 mb-1 border-bottom"><b>@Model.data.exchange_rate</b></h5>
			<p class="tx-12 text-muted">@Model.data.last_update_utc</p>
			<div class="card-icon-wrapper">
				<i class="fa-solid fa-dollar-sign"></i>
			</div>
		</div>
	</div>
</div>
